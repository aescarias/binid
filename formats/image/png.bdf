{
  meta: {
    bdf: "0.1",
    name: "Portable Network Graphics (PNG) image",
    mime: ["image/png"],
    exts: [".png"],
  },
  binary: [
    { type: magic, match: "\x89PNG\r\n\x1a\n" },
    // no repeat sequence yet for chunk parsing. the structure of a single chunk is: 
    { type: uint32,       id: length, endian: "big" },
    { type: byte[4],      id: type },
    { type: byte[length], id: data },
    { type: uint32,       id: crc32,  endian: "big" },
  ]
}
